---
import { Search, ShoppingBag, ArrowRight } from "lucide-react";
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<button
  class="flex items-center gap-3 px-7 py-2 bg-[#F83D8E] text-white shadow-[#F83D8E] shadow-md hover:bg-white duration-300 transition-all font-medium hover:text-black rounded-[30px]"
  >{title}<ArrowRight className="h-[20px]" /></button
>
